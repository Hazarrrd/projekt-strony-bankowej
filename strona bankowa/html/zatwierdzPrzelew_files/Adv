ader">\r\n        {{getText(\'oneClick.waitText\')}}\r\n    <\/div>\r\n<\/div>\r\n'});define("Adv/Scripts/OneClick/infrastructure/i18TextProvider",[],function(){var n=function(n,t){if(t==null)return I18n.t(n);var i=t+"."+n,r=I18n.t(i),u=I18n.missingTranslation(i);return r==u?I18n.t(n):r};return{getText:n}});define("Adv/Scripts/OneClick/views/cashCreditView",["text!Adv/Templates/OneClick/cashCredit.html","../infrastructure/i18TextProvider"],function(n,t){return Backbone.Marionette.ItemView.extend({template:n,tagName:"div",id:"oneclick-cash-credit",amountTimer:undefined,amountReady:!0,infoBoxTimeout:!1,infoBoxStatus:!1,infoBoxHidingTime:350,initialize:function(n){this.controller=n.controller;this.i18prefix=n.i18prefix},templateHelpers:function(){var i=this,n=function(){this.getText=function(n){return t.getText(n,i.i18prefix)}};return n.prototype=Backbone.Marionette.View.prototype.templateHelpers,new n},events:{"click #oc-KG-btn-accept":function(){if(this.amountReady)return this.hideInfoBox(0),this.showInMode("W"),this.saveData(),!1},"blur #oc-KG-inp-amount":"onAmountBlur","keypress  #oc-KG-inp-amount":"onAmountKeypress","click #oc-KG-amount-modification .btn-red-gradient":"onAmountModifiedConfirm","click #oc-KG-amount-modification .btn-gray-gradient":function(){return this.hideInfoBox(),this.controller.trigger("close"),!1}},onAmountKeypress:function(n){var r;if(n.altKey||n.ctrlKey||n.shiftKey)return!1;var u=$(n.currentTarget),t=n.which,i=n.keyCode;return t===13?(u.blur(),!0):t>=48&&t<=57||i===8||(i===37||i===39||i===46)&&t===0?(r=this,this.setAmountState(!1),this.amountTimer!=undefined&&clearTimeout(this.amountTimer),this.amountTimer=setTimeout(function(){r.setAmountValue(n)},2e3),!0):!1},setAmountState:function(n){this.amountReady!==n&&(this.amountReady=n,n?(this.$el.find("#oc-KG-btn-accept").css("cursor","pointer"),this.$el.find("#oc-KG-btn-accept").animate({opacity:"1"},200)):(this.$el.find("#oc-KG-btn-accept").css("cursor","default"),this.$el.find("#oc-KG-btn-acce